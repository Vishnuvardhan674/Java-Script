border-radius: 15px;
width: 650px;
font-size: 25px;
background-color: pink;
padding-left: 50px;
}
</style>
<body>
<div id="mydiv"></div>
<script>
function myprint(data){
var mydiv=document.getElementById("mydiv");
console.log(mydiv);
mydiv.innerHTML=mydiv.innerHTML+data+"<br/>";
}
myprint("<u>Switch statement<br/></u>") 

//var day=5;
var day = new Date().getDay();
var day=5;
switch (day) {
case 0:
myprint("Sunday");
break;
case 1:
myprint("Monday");
break;
case 2:
myprint("Tuesday");
break;
case 3:
myprint("Wednesday");
break;
case 4:
myprint("Thursday");
break;
case 5:
myprint("Friday");
break;
case 6:
myprint("Saturday");
break;
default:
myprint("Invalid");
} 
