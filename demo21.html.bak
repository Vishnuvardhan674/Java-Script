<!DOCKTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>If statement</title>
</head>
<style>
div{
margin: 50px;
border: 2px solid red;
border-radius: 15px;
width: 650px;
font-size: 25px;
background-color: blue;
padding-left: 50px;
}
</style>
<body>
<div id="mydiv"></div>
<script>
function myprint(data){
var mydiv=document.getElementById("mydiv");
console.log(mydiv);
mydiv.innerHTML=mydiv.innerHTML+data+"<br/>";
}
myprint("<u>If statement<br/></u>") 
myprint("<u>If statement</u>")

var a=10;

if (a > 10) {
myprint("a is greater than 10");
}

if(a==10){
myprint("a is equal to 10");
}
myprint("--------");
var a=10;
var b=20;
var max;

if(a>b){
max=a;
}
else{
max=b;
}
myprint("Max:"+max);

myprint("--------");
var a = 10;
var b = 20;
var c = 30;
var max;
if (a > b && a > c) {
max = a;
} else if ((b > c)) {
max = b;
} else {
max = c;
}
myprint("Max:"+max);

myprint("Done");

</script>
</body>
</html>